{
    "name": "Titiricfy",
    "version": "0.0.9",
    "manifest_version": 2,
    "description": "Replace all pictures of all pages for photo from Tiririca.",
    "icons": {
        "16": "images/Tiririca16.png",
        "128": "images/Tiririca128.png"
    },
    "default_locale": "en",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "scripts/jquery-1.8.2.min.js",
                "scripts/contentscript.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}